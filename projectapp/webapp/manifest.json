{
    "_version": "1.32.0",
    "sap.app": {
        "id": "ns.projectapp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "dataSources": {
            "northwind":{
                "uri":"../0315592f-f343-4c64-8997-0df32bec9a18.ns-projectapp.nsprojectapp/northwind/"
            },
            "northwind-local":{
                "uri":"http://localhost:8081/https://712c5580trial.launchpad.cfapps.us10.hana.ondemand.com/0315592f-f343-4c64-8997-0df32bec9a18.ns-projectapp.nsprojectapp/northwind/"
            }
        },
        "crossNavigation": {
            "inbounds": {
                "ns-projectapp-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "northwind2",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.100.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {},
				"sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ns.projectapp.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "JS",
                "async": true,
                "viewPath": "ns.projectapp.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": "RouteView1",
                    "target": [
                        "TargetView1",
                        "TargetView2"
                    ]
                },
                {
                    "name": "RouteView2",
                    "pattern": "RouteView2",
                    "target": [
                        "TargetView2",
                        "TargetView1"
                    ]
                }
            ],
            "targets": {
                "TargetView1": {
                    "viewName": "View1",
                    "ControlAggregation":"beginColumnPages"
                },
                "TargetView2": {
                    "viewName": "View1",
                    "ControlAggregation":"midColumnPages"
                }
            }
        },
        "rootView": {
            "viewName": "ns.projectapp.view.View1",
            "type": "JS",
            "async": true,
            "id": "View1"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "ns-projectapp"
    }
}